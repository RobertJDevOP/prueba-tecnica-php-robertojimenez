{
  "name": "robertojimenez/repository-pattern",
  "description": "An implementation of repository pattern example",
  "type": "project",
  "require": {
    "php": "^8.3",
    "ext-pdo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10",
    "friendsofphp/php-cs-fixer": "3.53.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts" : {
    "php-cs-fixer": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php --using-cache=no"
  },
  "minimum-stability": "stable",
  "license": "MIT"
}
